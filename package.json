{
  "name": "kokutalk-anonymous",
  "version": "1.0.0",
  "main": "expo-router/entry",
  "scripts": {
    "start": "expo start",
    "build:android": "npx eas-cli build -p android --profile preview",
    "offline": "expo start --offline",
    "publish:expo": "eas update --branch preview && npx sentry-expo-upload-sourcemaps dist",
    "publish:sentry": "npx sentry-expo-upload-sourcemaps dist",
    "upgrade": "npx expo upgrade",
    "android": "npx expo run:android",
    "test": "jest --watchAll",
    "lint": "eslint \"**/*.{js,jsx,ts,tsx}\" && prettier -c \"**/*.{js,jsx,ts,tsx,json}\"",
    "format": "eslint \"**/*.{js,jsx,ts,tsx}\" --fix && prettier \"**/*.{js,jsx,ts,tsx,json}\" --write",
    "ios": "expo run:ios"
  },
  "dependencies": {
    "@expo/vector-icons": "^14.0.0",
    "@react-navigation/drawer": "^6.6.15",
    "@react-navigation/native": "^6.1.7",
    "@sentry/react-native": "~5.20.0",
    "@shopify/flash-list": "1.6.3",
    "dayjs": "^1.11.11",
    "dotenv": "^16.4.5",
    "expo": "~50.0.17",
    "expo-application": "~5.8.4",
    "expo-blur": "~12.9.2",
    "expo-constants": "~15.4.5",
    "expo-crypto": "~12.8.1",
    "expo-device": "~5.9.4",
    "expo-image-picker": "~14.7.1",
    "expo-insights": "~0.6.1",
    "expo-linear-gradient": "~12.7.2",
    "expo-linking": "~6.2.2",
    "expo-notifications": "~0.27.7",
    "expo-router": "~3.4.10",
    "expo-system-ui": "~2.9.3",
    "expo-updates": "~0.24.12",
    "formik": "^2.4.6",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-native": "0.73.6",
    "react-native-gesture-handler": "~2.14.0",
    "react-native-keyboard-aware-scroll-view": "^0.9.5",
    "react-native-mmkv": "^2.12.2",
    "react-native-reanimated": "~3.6.2",
    "react-native-ruler-picker": "^0.2.2",
    "react-native-safe-area-context": "^4.8.2",
    "react-native-screens": "~3.29.0",
    "react-native-size-matters": "^0.4.2",
    "react-native-unistyles": "^2.1.1",
    "yup": "^1.4.0",
    "zustand": "^4.5.4"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@types/react": "~18.2.14",
    "@typescript-eslint/eslint-plugin": "^7.7.0",
    "@typescript-eslint/parser": "^7.7.0",
    "eslint": "^8.57.0",
    "eslint-config-universe": "^12.0.1",
    "eslint-plugin-hooks": "^0.4.3",
    "eslint-plugin-react": "^7.34.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "prettier": "^3.2.5",
    "typescript": "^5.4.5"
  },
  "eslintConfig": {
    "extends": "universe/native"
  },
  "resolutions": {
    "expo-modules-core": "~1.11.0"
  },
  "overrides": {
    "expo-modules-core": "~1.11.0"
  },
  "expo": {
    "install": {
      "exclude": [
        "react-native-safe-area-context"
      ]
    }
